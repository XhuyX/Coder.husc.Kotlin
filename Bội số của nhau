import kotlin.math.sqrt

fun main() {
    var x= readLine()?.split(" ")
    if (x!=null){
        var a= x[0].toLong()
        var b =x[1].toLong()
        if (a==0.toLong() || b==0.toLong()){
            print("Yes")
        }
        else{
            if (a%b==0.toLong() ||b%a==0.toLong()){
                print("Yes")
            }
            else{
                print("No")
            }
        }
    }
}
