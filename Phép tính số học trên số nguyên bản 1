fun main(args: Array<String>) {
    var n= readLine()?.split(" ")
    if (n!=null){
        var a = n[0].toInt()
        var b = n[1].toInt()
        print("$a + $b = ")
        println(a+b)
        print("$a - $b = ")
        println(a-b)
        print("$a * $b = ")
        println(a*b)
        print("$a / $b = ")
        println(a/b)
        print("$a % $b = ")
        println(a%b)
    }
}
